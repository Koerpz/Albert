<Window x:Class="Leo.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Leo"
        mc:Ignorable="d"
        Title="SettingsWindow" Height="450" Width="800">
    <Grid>
        <!-- Links de menuopties -->
        <StackPanel Height="1000" Width="150" VerticalAlignment="Top" HorizontalAlignment="Left" Background="gray">
            <Button Click="Audio_Click" Content="Audio" Margin="10"/>
            <Button Content="Graphics" Margin="10"/>
            <Button Content="General" Margin="10"/>
            <Button Content="Back" Margin="10" VerticalAlignment="Bottom"/>
        </StackPanel>
        <Frame x:Name="SettingsBlanco" Margin=" 150, 0, 0, 0" NavigationUIVisibility="Hidden"/>
    </Grid>
    
</Window>
